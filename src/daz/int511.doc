 procedure int511PROC12(A, B, C : in out BOOLEAN)
 is
	X : BOOLEAN;
 begin
    X := A and B;
    A := X and then B;
    B := X or B;
    X := A or else B;
    A := A xor B;
    A := ((A and B) or C) and (B xor A);
 end int511PROC12;
