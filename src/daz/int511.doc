 procedure int511PROC6(X, Y : in out INTEGER)
 is
    type IND is range 1 .. 2;
    type ARR is array(IND) of INTEGER;
    type REC is record X, Y : INTEGER; end record;
    A : ARR;
    R : REC;
    procedure P (Z : INTEGER) is
