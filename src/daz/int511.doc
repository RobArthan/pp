    then X := 9;
    else Y := 42;
    end if;
    if X > Y
    then X := 42;
    elsif X < Y
    then Y := 9;
    end if;
    P(Z => X);
    Q(X, Y);
    R.X := X;
    R.Y := Y;
 end int511PROC6;
