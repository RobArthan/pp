fun €generate_ed_line› (offset : int, label : string) = (
	if offset < 0 (* § is a plug *)
	then
		[
		"s/^\\\\+/\\\\+\\\\FTHyperRefPlug{"^label^"} /\n"
		]
	else
		skip_fwd offset@
		[
		"s/\\\\\\\\$/ \\\\FTHyperRefSocket{"^label^"}\\\\\\\\/\n"
		]
);
