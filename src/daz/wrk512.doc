=SML
val cn_auto_tac = (
	FIRST (map SOLVED_T [
		cn_auto_tac1,
		cn_auto_tac2,
		cn_auto_tac3
	])
);
=TEX
=SML
fun try_vc vc_name = (
	let	val cnj = get_conjecture"-" vc_name;
		val thm = tac_proof(([], cnj), cn_auto_tac);
	in	(save_thm(vc_name, thm); ())
		handle Fail _ => (diag_line ("Cannot save:" ^ vc_name))
	end 
);
=TEX
=SML
fun try_vcs (vcs : string list) : string list = (
	let	fun aux [] = []
		|   aux (vcn ::more) = (
			(	try_vc vcn;
				diag_line ("Proved: " ^ vcn);
				aux more
			)	handle Fail _ => (	
				diag_line ("Failed to prove: " ^ vcn);
				vcn :: aux more
			)
		);
	in	aux vcs
end
);
=TEX
=SML
val vc_names = (rev o flat o map fst o get_conjectures)"-";
val unsolved_vcs = try_vcs vc_names;
=TEX
