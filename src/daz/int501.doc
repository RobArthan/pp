\subsection{Subunits}
\subsubsection{Function as Subunit}
Note that a function stub and the corresponding subunit cannot appear in the same literate script.
=SML
open_theory"daz_toolkit_extensions";
CNZGenerator.new_literate_script"int501D1";
=TEX
¹CN
procedure FNC_SBNT (A : INTEGER; B : out INTEGER)
is
	I : INTEGER;
	function  TIMES_2_I (X : INTEGER) return INTEGER
	„ [TIMES_2_I(X) = I * X + I * X]
	is
	separate;
begin
	I := A;
	B := TIMES_2_I(A);
end FNC_SBNT;
°
=SML
open_theory"daz_toolkit_extensions";
CNZGenerator.new_literate_script"int501D2";
=TEX
¹CN
separate (FNC_SBNT)
function TIMES_2_I (X : INTEGER) return INTEGER
	„ [TIMES_2_I(X) = 2*I*X]
is
begin
	return 2*I*X;
end TIMES_2_I;
°

\pagebreak
\subsubsection{Procedure as Subunit}
=SML
open_theory"daz_toolkit_extensions";
CNZGenerator.new_literate_script"int501E";
=TEX
¹CN
procedure PRC_SBNT (A : INTEGER; B : out INTEGER)
is
	I : INTEGER;
	procedure MULT_BY_2_I (X : INTEGER; Y : out INTEGER)
	„ Y[Y = I * X + I * X]
	is
	separate;
begin
	I := A;
	MULT_BY_2_I(A, B);
end PRC_SBNT;
°
¹CN
separate (PRC_SBNT)
procedure MULT_BY_2_I (X : INTEGER; Y : out INTEGER)
	„ Y[Y = 2*I*X]
is
begin
	Y := 2*I*X;
end MULT_BY_2_I;
°
