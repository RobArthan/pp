¹CN
procedure FNC_SBNT (A : INTEGER; B : out INTEGER)
is
	I : constant  INTEGER := 99;
	function  TIMES_2_I (X : INTEGER) return INTEGER
	˜ [TIMES_2_I(X) = I * X + I * X]
	is
	separate;
begin
	B := A;
	B := TIMES_2_I(B);
end FNC_SBNT;
