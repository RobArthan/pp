\subsection{Refinement}

=SML
open_theory"cn";
new_script{name="int501H", state=int501G_state};
=TEX

πCN
procedure EG1 (X : in out INTEGER; Y : in out INTEGER)
is
	T : INTEGER;
begin
	Ñ T [ X  > 1 ± Y > 1 ¥ X + Y < T < X * Y]		(10)
end EG1;
∞
πCN
(10) √
	T := X + Y + 1;
∞
πCN
procedure EG2 (X : in out INTEGER)
is
	T : INTEGER;
begin
	Ñ T [ X  < 1 ¥ T > X]
end EG2;
∞
πCN
√
	T := 1;
∞

