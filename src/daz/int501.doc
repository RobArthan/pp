\subsection{Subprograms in Packages}
\subsubsection{Functions in Packages}

=SML
open_theory"cn";
new_script{name="int501A", state=initial_cn_state};
=TEX

πCN
package FNC_IN_PCK is

	function LOWER_BOUND (M : INTEGER) return INTEGER
	Ñ [LOWER_BOUND(M) < M];

end FNC_IN_PCK;
∞
πCN
package body FNC_IN_PCK is

	function LOWER_BOUND (M : INTEGER) return INTEGER
	Ñ [LOWER_BOUND(M) = M - 1]
	is
	begin
		return M - 1;
	end LOWER_BOUND;

end FNC_IN_PCK;
∞

=SML
