package body CALCULATOR is

   $using S, N : INTEGER; $implement B $by Bag_Inv;

   procedure CLEAR
   „ S, N [N = 0 ´ S = 0, S = 0 ± N = 0]
   is
   begin
      if N = 0 then null;
      else S := 0; N := 0;
      end if;
   end CLEAR;

   procedure ENTER (X : in INTEGER)
   „ S, N [S = S‰0 + X ± N = N‰0 + 1]
   is
   begin
      S := S + X;
      N := N + 1;
   end ENTER;

   procedure MEAN (M : out INTEGER)
   „ M, S, N [N ½ 0, M = S intdiv N ± S = S‰0 ± N = N‰0]
   is
   begin
      M := S / N;
   end MEAN;


begin
   S := 0;

end CALCULATOR;
