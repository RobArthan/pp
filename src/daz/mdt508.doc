val _ = store_mt_results (mt_runf (op =)) [
("bd2", bdtest, ÒPAY_DAY = FRIÆ, true)];
=TEX
\subsection{Translating Array Types}
=SML
val bd3 = cn_declare "type ARR is array (DAY) of DAY;";
val bd4 = cn_declare "type UNCON is array (DAY range <>) of DAY;";
