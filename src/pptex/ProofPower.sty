\def\make@vert@bar{%
	\ifdim \dp\@line > \dp\ft@strutbox
		\@A=\dp\@line
		\else \@A=\dp\ft@strutbox
	\fi
	\advance\@A by\lineskip
	\ifdim \ht\@line > \ht\ft@strutbox
		\@B=\ht\@line
		\else \@B=\ht\ft@strutbox
	\fi
	\setbox\@block=\hbox{\vrule height\@B depth\@A\thinspace}%
}
