fun Ûz_const_specÝ (znam : string):unit =
let	val thm_reqd = get_flag"defining_theorem";
	val (ztm,deftm) = name_to_termpr znam
		handle complaint =>
		pass_on complaint "name_to_termpr""z_const_def";
