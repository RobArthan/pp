	val conj = list_mk_±(map (fn pr => mk_app(pr,t))get_dpprojs);
	val absttm = mk_Ì(t,conj);
	val absxtm = mk_Ì(x,mk_app(absttm,substm));
	val rhs = match_mk_app(¬$"Z'Absð"®,absxtm);
in
	(schtm,mk_µ(p,mk_eq(lhs,rhs)))
