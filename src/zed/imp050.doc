			| _ => fail "get_z_const_defn" 0 []; 
	val tym = type_match zty pzty;
	val thm = get_defn "Z" znam
	handle complaint =>
		pass_on complaint "get_defn""get_z_const_defn";
in	inst_type_rule tym thm
end;
