=SML
  | ti_z_tm env (term as ZTmFancyApp (id as (n, d), tma)) = (
	case get_zfixity_info (first_tmpl_id n) of
	Value((_, ZClGen _),_) => (
		case tma of
		ZTmTuple tml => ti_z_tm env (ZTmLVar (id, Nil, tml))
		| _ => fail "ti_z_tm" 62901 [fn()=>"(gen) Bad Generic argument"]
		)
	|Value((_, ZClRel),_) => (
