=SML

fun 	Ûval_setIDENTÝ (sl:string list) :string =
	"[" ^ flatpack(idsetseq sl) ^ "]";

fun	 ÛiextymfunseqÝ (itym:(string * EXTYPE)list) :EXTYPE list =
	map (lassoc3 itym) (idsetseq(fst(split itym)));
