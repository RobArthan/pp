\subsection{Local Variables}
=SML
 | ti_ztm env (term as ZTmLVar (id as (n, d), tml)) = (
let	val (is_con, ty) = env id;
in
	if is_con andalso d <> "" andalso (case manifest ty of
				SchemaType _ => true
			|	_ => false)
	then ti_ztm env (ZTmDecor (ZTmLVar((n, ""), tml), d))
	else if is_con
	then  ti_ztm env (ZTmGVar (id, tml))
	else (
		case tml of	[] => (
		(ZTmLVar(id, []), ty)
		)|_ =>
			let	val ttmlist = list_ti_ztm env tml;
				val (ttml,tyl) = split ttmlist
				val argty = tuple_extype tyl;
				val tvn = next_name();
				val resty = ExVartype(Unknown tvn);
				val side = unify zti_subs (ty, ex_ (tuple_extype [argty, resty]))
					handle Fail msg =>
					error "ti_ztm" 62998 [fn ()=>
					"ZTmLVar (message to be supplied later)"];
				val ty' = sub_type zti_subs ty;
			in
				(ZTmLVar(id, ttml), ty')
			end)
end
)
=TEX

\subsection{Global Variables}

=SML
 | ti_ztm env (term as ZTmGVar (id as (n, d), tml)) = (
	case (env id, tml, id) of 
	((true, ExType("≠",[domty,ranty])), [], (TmplNon ("U", Nil),"")) => 
		let	val (bnam,gentyl) = (
				case domty of
				ExType(b, l) => (b, l)
				| _ => error "ti_ztm" 62998 [fn()=>"Bad generic constant"]);
			val ztml' = map (fn x => ZTmGVar((TmplNon ("totality",Nil),""),[])) gentyl;
		in
			ti_ztm env (ZTmGVar(id,ztml'))
		end
	| ((true, ExType("≠",[domty,ranty])),[],_) => 
		let	val (bnam,gentyl) = (
				case domty of
				ExType(b,l) => (b,l)
				| _ => error "ti_ztm" 62998 [fn()=>"Bad generic constant"]);
			val ztml' = map (fn x => ZTmGVar((TmplNon("U",Nil),""),[]))
									 gentyl;
		in
			ti_ztm env (ZTmGVar(id, ztml'))
		end
	| ((true,ty as ExType("≠",[domty,ranty])), _, _) =>
		let	val tmtyl = list_ti_ztm env tml;
			val argty = tuple_extype ((map snd) tmtyl);
			val tvn = next_name();
			val resty = ExVartype(Unknown tvn);
			val side = unify zti_subs (ExType ("≠",[domty, ranty]),
				ExType ("≠", [argty, resty]))
			in
				(ZTmGVar(id,map fst tmtyl), sub_type zti_subs ty)
			end
	| ((true,ty),[],_) =>	(ZTmGVar(id,[]), ty)
	|_ => error "ti_ztm" 62998 [fn()=>"ZTmGVar: Not a constant"]
)
=TEX

\subsection{Let Expressions}

=SML
 | ti_ztm env (term as ZTmLet (eqdefl, tm)) = (
let	fun rep_unify ((ty1, ty2)::tyl) = (
	let	val sd = unify zti_subs (ty1, ty2)
		 handle Fail msg =>
			raise ZExn (FailedZTm (term, env, Failed));

	in	rep_unify tyl
	end)
		  | rep_unify _ = ();

	fun get_tys env (id::rest) = (
		case env id of
			(true, _) => raise ZExn (FailedZTm (term, env, Failed))
			|(false, ty) => (id, ty)::(get_tys env rest)
	) | get_tys env [] = [];

	val (abbl, defl) = split eqdefl;
	val (abbl', atyl') = split (get_tys env abbl);
	val deftyl = list_ti_ztm env defl;
	val (defl', tyl') = split deftyl;
	val side = rep_unify (combine atyl' tyl');
	val eqdefl' = combine abbl' defl';
	val env' = sub_ty env;
	val (tm', ty') = ti_ztm env' tm;
in
	(ZTmLet (eqdefl', tm'), ty')
end
)
=TEX

