\subsection{Schema Composition}

=SML
 | ti_ztm env (term as ZTmª (tm1, tm2)) = (
let	fun f [] [] _ = []
	  | f [] (h::t) tym1 = (
		case h of
		((s,""),ty2) => (
			case lassoc5 tym1 (s,"'") of
			Value ty1 => (
				let	val dl = f [] t tym1
					val side = unify zti_subs (ty1, ty2)
				in
					dl
				end)
			| Nil => raise ZExn (FailedZTm (term, env, Failed)))
		| _ => f [] t tym1)
	  | f (h::t) tym2 tym1 = (
		case h of
		((s,"'"),ty1) => (
			case lassoc5 tym2 (s,"") of
			Value ty2 => (
				let	val dl = f t tym2 tym1
					val side = unify zti_subs (ty1, ty2)
					in
						(s,"")::dl
					end)
			| Nil => raise ZExn (FailedZTm (term, env, Failed)))
		| _ => f t tym2 tym1);
	val (tm1', ty1') = ti_ztm env tm1;
	val (tm2', ty2') = ti_ztm (sub_ty env) tm2;
in
	case (manifest ty1', manifest ty2') of
	(SchemaType idtyl1,SchemaType idtyl2) => (
		let	val dropl = f idtyl1 idtyl2 idtyl1;
			val idtyl1' = idtyl1 drop (fn ((s,d),ty) => (d = "'")
					andalso (s,"") mem dropl);
			val idtyl2' = idtyl2 drop (fn ((s,d),ty) => (d = "")
					andalso (s,"") mem dropl);
			val idtym = idtyl1' cup idtyl2';
			val cty = ex_ (bind_extype  idtym);
		in
			(ZTmª(tm1',tm2'), cty)
		end)
	| _ => raise ZExn (FailedZTm (term, env, Failed))
end)
=TEX

\subsection{Schema Piping}

=SML
 | ti_ztm env (term as ZTmPipe (tm1, tm2)) = (
let	fun f [] [] _ = []
	  | f [] (h::t) tym1 = (
		case h of
		((s,"?"),ty2) => (
			case lassoc5 tym1 (s,"!") of
			Value ty1 => (
				let	val dl = f [] t tym1;
					val side = unify zti_subs (ty1, ty2);
				in
					dl
				end)
			| Nil => raise ZExn (FailedZTm (term, env, Failed)))
		| _ => f [] t tym1)
	  | f (h::t) tym2 tym1 = (
		case h of
		((s,"!"),ty1) => (
			case lassoc5 tym2 (s,"?") of
			Value ty2 => (
				let	val dl = f t tym2 tym1;
					val side = unify zti_subs (ty1, ty2);
					in
						(s,"")::dl
					end)
			| Nil => raise ZExn (FailedZTm (term, env, Failed)))
		| _ => f t tym2 tym1);
	val (tm1', ty1') = ti_ztm env tm1;
	val (tm2', ty2') = ti_ztm (sub_ty env) tm2;
in
	case (manifest ty1', manifest ty2') of
	(SchemaType idtyl1, SchemaType idtyl2) => (
		let	val dropl = f idtyl1 idtyl2 idtyl1;
			val idtyl1' = idtyl1 drop (fn ((s,d),ty) => (d = "!")
					andalso (s,"") mem dropl);
			val idtyl2' = idtyl2 drop (fn ((s,d),ty) => (d = "?")
					andalso (s,"") mem dropl);
			val idtym = idtyl1' cup idtyl2';
			val cty = ex_ (bind_extype  idtym);
		in
			(ZTmPipe (tm1', tm2'), cty)
		end)
	| _ => raise ZExn (FailedZTm (term, env, Failed))
end)
=TEX

