=SML
fun	ÛidsetseqÝ (sl:string list) : string list = Sort.sort Sort.string_order sl;
=TEX
=SML
fun	ÛflatpackÝ ([]:string list) : string 	= ""
|	flatpack (s::[])			= s
|	flatpack (s::ss) 			= s ^ "," ^ (flatpack ss);
