=SML
fun €chartuple› ((decls, tm) : Z_SCH_TEXT) : Z_TM = (
let	fun f (ZDec(ids, tm)) = (
		map (fn id => ZTmLVar(id,[])) ids
	) | f (ZSchemaDec (ZTmDecor (tm1, d))) = (
		[ZTm (tm1, d)]
