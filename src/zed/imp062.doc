local
fun f [] tym1 tym2 = Value []
  | f (c::cl) tym1 tym2 = (
	case f cl tym1 tym2 of 
	Nil => Nil
	|Value tym => (
		case (lassoc5 tym1 c,lassoc5 tym2 c) of
		(Nil,Value ty) => Value((c,ty)::tym)
