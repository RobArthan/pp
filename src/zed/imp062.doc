in
	ZInferred (ZParaTerm (formals, tm', Value ty'))
end
=TEX
=SML
) | ti_z_para (p as ZParaTerm (_, _, Value _)) = (
	error "ti_z_para" 62998 [fn()=>"The term passed to the type inferrer must not be typed"]
=TEX
