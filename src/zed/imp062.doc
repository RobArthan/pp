end)
=TEX
=SML
  | ti_z_tm env (term as ZTmSeta2 (schtext, tm)) = (
let	val (schtext', env', _) = ti_z_schtext env schtext
		handle ZExn (FailedZSchText (_, env, tm', ty')) =>
