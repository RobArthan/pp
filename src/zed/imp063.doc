val Ûterm_of_z_tmÝ : Z_TM -> TERM = do_gvar_subst o tm_of_z_tm o do_types_to_define;
val Ûlist_term_of_z_tmÝ : Z_TM list -> TERM list = map do_gvar_subst o list_tm_of_z_tm o do_types_to_define;
