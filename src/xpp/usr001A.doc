\section{DISPLAYING KEYBOARDS}

=TEX
\long\def\TypeiiKeyboard{{
	\makeatletter\def\@moan##1{\nuldef}\makeatother
	\UNITHEIGHT=2\UNITHEIGHT
	\ShowAllImages
	\let\KEY=\LONGSHOWKEY
	\let\SHIFT=\LONGSHIFTKEY
	\KEYBOARDii
}}

\long\def\TypeiiiKeyboard{{
	\makeatletter\def\@moan##1{\nuldef}\makeatother
	\UNITHEIGHT=2\UNITHEIGHT
	\ShowAllImages
	\let\KEY=\LONGSHOWKEY
	\let\SHIFT=\LONGSHIFTKEY
	\KEYBOARDiii
}}

\long\def\TypeivKeyboard{{
	\makeatletter\def\@moan##1{\nuldef}\makeatother
	\UNITHEIGHT=2\UNITHEIGHT
	\ShowAllImages
	\let\KEY=\LONGSHOWKEY
	\let\SHIFT=\LONGSHIFTKEY
	\KEYBOARDiv
}}

=TEX
\long\def\SeparatedKeySets#1{
\def\Header##1##2{Type ##1 keyboard --- #1 --- \okeyboardVersion\hfill ##2\par\smallskip}

\let\SHIFT=\KEY
\Header 2{META}			\let\KEY=\KeyM	\let\SHIFT=\KEY	\KEYBOARDii\vfil
\Header 2{META + SHIFT}		\let\KEY=\KeyMS	\let\SHIFT=\KEY	\KEYBOARDii\vfil
\Header 2{META + CONTROL}	\let\KEY=\KeyMC	\let\SHIFT=\KEY	\KEYBOARDii

\newpage

\Header 3{META}			\let\KEY=\KeyM	\let\SHIFT=\KEY	\KEYBOARDiii\vfil
\Header 3{META + SHIFT}		\let\KEY=\KeyMS	\let\SHIFT=\KEY	\KEYBOARDiii\vfil
\Header 3{META + CONTROL}	\let\KEY=\KeyMC	\let\SHIFT=\KEY	\KEYBOARDiii

\newpage

\Header 4{META}			\let\KEY=\KeyM	\let\SHIFT=\KEY	\KEYBOARDiv\vfil
\Header 4{META + SHIFT}		\let\KEY=\KeyMS	\let\SHIFT=\KEY	\KEYBOARDiv\vfil
\Header 4{META + CONTROL}	\let\KEY=\KeyMC	\let\SHIFT=\KEY	\KEYBOARDiv
}
=IGN

