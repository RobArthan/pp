\def	\sKix	{\K (9{\PrLJ}{\PrKI}{\PrLJ}{}}
\def	\sKx 	{\K )0{\PrLA}{\PrKJ}{\PrLA}{}}
\def\sNumerics{\sKi\sKii\sKiii\sKiv\sKv\sKvi\sKvii\sKviii\sKix\sKx\relax}

\def	\sKQ	{\K Q{}{\PrPB}{\PrNB}{\PrJB}{}}
