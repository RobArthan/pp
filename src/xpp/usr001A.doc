=TEX
\long\def\MergedKeySets#1{
\def\Header##1{\newpage Type ##1 keyboard --- #1 --- \okeyboardVersion\par\smallskip}

\Header2	\let\KEY=\LONGSHOWKEY	\let\SHIFT=\LONGSHIFTKEY	\KEYBOARDii
\Header3	\let\KEY=\LONGSHOWKEY	\let\SHIFT=\LONGSHIFTKEY	\KEYBOARDiii
\Header4	\let\KEY=\LONGSHOWKEY	\let\SHIFT=\LONGSHIFTKEY	\KEYBOARDiv
}
