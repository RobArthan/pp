structure ‚InitTheory· : InitTheory = struct
local
	val unit = new_theory "init";
in

	val BOOL_CASES = new_axiom ("BOOL_CASES",
		ªÉb é (b = T) Å (b = F)º);

	val IMP_ANTISYM = new_axiom ("IMP_ANTISYM",
		ªÉb1 é Éb2 é (b1 ä b2) ä (b2 ä b1) ä (b1 = b2)º);

	val ETA = new_axiom ("ETA",
		ªÉf:'a ã 'b é (çx é f x) = fº);

	val á = new_axiom ("á",
		ªÉP: 'a ã BOOL é Éx é P x ä P($á P)º);

	val INFINITY = new_axiom ("INFINITY",
		ªÑf:IND ã IND é ONE_ONE f Ä å(ONTO f)º);
end; (* of local ... in ... *)
end; (* of structure InitTheory *)
