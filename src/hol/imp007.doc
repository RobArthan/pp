fun ÛRANDS_CÝ (cnv : CONV) : CONV = (fn (tm:TERM) =>
let	val (f,xs) = strip_app tm;
	val xs' = map cnv xs;
	val s2 = (if is_nil xs
		then refl_conv f
		else list_mk_app_rule (app_fun_rule f (hd xs')) (tl xs')
		handle complaint =>
		divert complaint "app_fun_rule" "RANDS_C" 7104 
			[fn () => string_of_thm (hd xs')]);
in
	s2
end);
end;
