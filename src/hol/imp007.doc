fun ÛRIGHT_CÝ (cnv : CONV) : CONV = (fn (tm:TERM) =>
let	val (fa,b) = (dest_app tm)
		handle (Fail _) =>
		term_fail "RIGHT_C" 3013 [tm];
	val dummy = if is_app fa
		then ()
		else term_fail "RIGHT_C" 3013 [tm];
	val s1 = cnv b;
	val s2 = app_fun_rule fa s1
		handle complaint =>
